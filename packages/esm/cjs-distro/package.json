{
  "name": "esm-with-cjs-distro",
  "version": "1.0.0",
  "description": "Example of a package with ESM source-code that distributes CJS code.",
  "type": "module",
  "engines": { "node": ">=12.22.7" },
  "exports": {
    ".": "./dist/main.cjs",
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "webpack --config=../webpack.config-cjs.cjs"
  },
  "keywords": [
    "cjs",
    "configuration",
    "esm",
    "modules",
    "nodejs"
  ],
  "author": "Jacob Smith",
  "license": "ISC",
  "devDependencies": {
    "webpack": "5.63.0",
    "webpack-cli": "4.9.1"
  }
}
